{
    "name": "scott-lsi/laravelshoppingbasket",
    "description": "Laravel 5 and above Shopping basket",
    "keywords": ["laravel", "shopping basket", "basket", "shopping basket", "basket"],
    "license": "MIT",
    "authors": [
        {
            "name": "Darryl Fernandez",
            "email": "engrdarrylfernandez@gmail.com"
        },
        {
            "name": "Rob Allport",
            "email": "rob@f9web.co.uk"
        },
        {
            "name": "Scott Brown",
            "email": "scott@scottbrown.me.uk"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/validation": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/translation": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^9.5"
    },
    "autoload": {
        "psr-4": {
            "ScottLsi\\": "src/ScottLsi"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "ScottLsi\\Basket\\BasketServiceProvider"
            ],
            "aliases": {
                "Basket": "ScottLsi\\Basket\\Facades\\BasketFacade"
            }
        }
    }
}
