{
    "name": "scott-lsi/laravelshoppingbasket",
    "description": "Laravel Shopping Basket",
    "keywords": ["laravel", "shopping basket", "basket", "shopping cart", "cart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Darryl Fernandez",
            "email": "engrdarrylfernandez@gmail.com"
        },
        {
            "name": "Rob Allport",
            "email": "rob@f9web.co.uk"
        },
        {
            "name": "Scott Brown",
            "email": "scott@scottbrown.me.uk"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0",
        "illuminate/validation": "^10.0",
        "illuminate/translation": "^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ScottLsi\\": "src/ScottLsi"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "ScottLsi\\Basket\\BasketServiceProvider"
            ],
            "aliases": {
                "Basket": "ScottLsi\\Basket\\Facades\\BasketFacade"
            }
        }
    }
}
